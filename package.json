{
  "name": "pepup-automation-gui",
  "version": "1.0.0",
  "description": "pepup automation GUI",
  "main": "src/index.ts",
  "engines": {
    "node": "10.15.x"
  },
  "dependencies": {
    "@types/gapi.client.drive": "^3.0.7",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-favicon": "^2.0.19",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^14.0.11",
    "@types/puppeteer": "^2.1.1",
    "axios": "^0.19.2",
    "googleapis": "^39.2.0",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-favicon": "^2.1.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.8",
    "koa-static": "^5.0.0",
    "moment": "^2.29.4",
    "node-config-ts": "^3.0.4",
    "puppeteer": "^3.3.0",
    "puppeteer-core": "^3.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "watch": "nodemon",
    "build": "npm run clean && npm run build-typescript && npm run cp-statics",
    "clean": "rm -rf ./dist",
    "build-typescript": "tsc",
    "cp-statics": "cp -r ./src/views ./dist/views",
    "test": "jest",
    "postinstall": "node-config-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gizumon/pepup-automation-gui.git"
  },
  "author": "tomoatsu.sekikawa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gizumon/pepup-automation-gui/issues"
  },
  "homepage": "https://github.com/gizumon/pepup-automation-gui#readme"
}
